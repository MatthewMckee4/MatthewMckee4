{
    "name": "matthew-mckee",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "type-check": "tsc --noEmit",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "prepare": "husky"
    },
    "dependencies": {
        "next": "14.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@types/node": "^20.11.19",
        "@types/react": "^18.2.57",
        "@types/react-dom": "^18.2.19",
        "@typescript-eslint/eslint-plugin": "^8.32.0",
        "@typescript-eslint/parser": "^8.32.0",
        "autoprefixer": "10.4.14",
        "eslint": "^8.56.0",
        "eslint-config-next": "^15.3.1",
        "eslint-config-prettier": "^10.1.2",
        "husky": "^9.1.7",
        "lint-staged": "^15.5.1",
        "postcss": "8.4.31",
        "prettier": "^3.5.3",
        "tailwindcss": "3.3.0",
        "tsc-files": "^1.1.4",
        "typescript": "^5.3.3"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "prettier --write",
            "eslint --fix",
            "tsc-files --noEmit"
        ],
        "*.{json,md}": [
            "prettier --write"
        ]
    }
}
